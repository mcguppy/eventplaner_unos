<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <el-resolver>org.mcguppy.eventplaner.jsf.util.JsfCrudELResolver</el-resolver>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>staffMemberController</managed-bean-name>
        <managed-bean-class>org.mcguppy.eventplaner.jsf.StaffMemberController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>staffMemberJpa</managed-bean-name>
        <managed-bean-class>org.mcguppy.eventplaner.jpa.controllers.StaffMemberJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>shiftController</managed-bean-name>
        <managed-bean-class>org.mcguppy.eventplaner.jsf.ShiftController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>shiftJpa</managed-bean-name>
        <managed-bean-class>org.mcguppy.eventplaner.jpa.controllers.ShiftJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>locationController</managed-bean-name>
        <managed-bean-class>org.mcguppy.eventplaner.jsf.LocationController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>locationJpa</managed-bean-name>
        <managed-bean-class>org.mcguppy.eventplaner.jpa.controllers.LocationJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <navigation-case>
            <from-outcome>staffMemberList</from-outcome>
            <to-view-id>/pages/staffMember/staffMemberList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>staffMemberCreate</from-outcome>
            <to-view-id>/pages/staffMember/staffMemberCreate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>staffMemberDetail</from-outcome>
            <to-view-id>/pages/staffMember/staffMemberDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>staffMemberEdit</from-outcome>
            <to-view-id>/pages/staffMember/staffMemberEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>staffMemberDestroy</from-outcome>
            <to-view-id>/pages/staffMember/staffMemberDestroy.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>shiftList</from-outcome>
            <to-view-id>/pages/shift/shiftList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>shiftCreate</from-outcome>
            <to-view-id>/pages/shift/shiftCreate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>shiftDetail</from-outcome>
            <to-view-id>/pages/shift/shiftDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>shiftEdit</from-outcome>
            <to-view-id>/pages/shift/shiftEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>shiftDestroy</from-outcome>
            <to-view-id>/pages/shift/shiftDestroy.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>locationList</from-outcome>
            <to-view-id>/pages/location/locationList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locationCreate</from-outcome>
            <to-view-id>/pages/location/locationCreate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locationDetail</from-outcome>
            <to-view-id>/pages/location/locationDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locationEdit</from-outcome>
            <to-view-id>/pages/location/locationEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locationDestroy</from-outcome>
            <to-view-id>/pages/location/locationDestroy.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
